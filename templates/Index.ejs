<!DOCTYPE html>
<html>
<head>
<style>
#customers {
  font-family: Arial, Helvetica, sans-serif;
  border-collapse: collapse;
  width: 100%;
}

#customers td, #customers th {
  border: 1px solid #ddd;
  padding: 8px;
}

#customers tr:nth-child(even){background-color: #f2f2f2;}

#customers tr:hover {background-color: #ddd;}

#customers th {
  padding-top: 12px;
  padding-bottom: 12px;
  text-align: left;
  background-color: #04AA6D;
  color: white;
}
</style>

</head>
<body>

<table id="customers">
  <tr>
    <th>Sr Number</th>
    <th>Instance</th>
    <th>Ip</th>
  </tr>
  <% for(var i=0; i<OriginalData.length; i++) {%>
    <tr>
        <td><%= i + 1%></td>
        <td><%= OriginalData[i].Instance%></td>
        <td><a style="text-decoration: none; color: black;" target="_blank" href="http://<%- OriginalData[i].Ip -%>/"><%=OriginalData[i].Ip%></a></td>
    </tr>
<% } %>
</table>

</body>
</html>
